{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "eri-adf-git"
		},
		"pipeline_Wait2_properties_0_typeProperties_waitTimeInSeconds": {
			"type": "int"
		},
		"pipeline_wait_properties_0_typeProperties_waitTimeInSeconds": {
			"type": "string"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "eri-adf-git_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"pipeline_Wait2_properties_0_typeProperties_waitTimeInSeconds": {
						"value": "[parameters('pipeline_Wait2_properties_0_typeProperties_waitTimeInSeconds')]"
					},
					"pipeline_wait_properties_0_typeProperties_waitTimeInSeconds": {
						"value": "[parameters('pipeline_wait_properties_0_typeProperties_waitTimeInSeconds')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}